#include<iostream>
using namespace std;

int main(){
	//Programme that defines the operator
	string name;
	int age;
	cout<<"Enter your name: "<<endl;
	cin>>name;
	cout<<"Enter your age :"<<endl;
	cin>>age;
	cout<<"Hello guys,My name is "<<name<<" and I am "<<age<<" years old."<<endl;
	
	
	
	
	return 0;
}
