#include<iostream>
using namespace std;

int main(){
	int a[5];
	int sum,i,num;
	num=7;
	sum = 0;
	for(i=0;i<num;i++){
		cout<<"Enter the value of a["<<i+1<<"]"<<endl;
		cin>>a[i];
	}
	for(i=0;i<num;i++){
		sum = sum + a[i];
	}
	double avg;
	avg = sum/num;
	cout<<"The average is "<<avg;
	
	return 0;
}
