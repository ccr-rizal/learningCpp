#include<iostream>
using namespace std;

class circle{
	
	public:

		double radius;
	
	double find_area(){
		double area = 3.14 * radius * radius;
		return area;
	}
	
};




int main(){
	
	circle c1,c2,c3;
	c1.radius = 2;
	c2.radius = 9.08;
	c3.radius = 6.96;
	
	double AreaOfC1 = c1.find_area();
		double AreaOfC2 = c2.find_area();
	double AreaOfC3 = c3.find_area();

	cout<<"The area of C1 is "<<AreaOfC1;
	
	return 0;
}
